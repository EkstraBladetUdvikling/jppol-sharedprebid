{
  "name": "jppol-sharedprebid",
  "version": "4.13.2",
  "description": "prebid for eb",
  "main": "index.js",
  "scripts": {
    "build": "node nodescripts/buildprebid.js --env=dev",
    "build:all": "node nodescripts/buildprebid.js --env=all",
    "build:prod": "node nodescripts/buildprebid.js --env=prod",
    "build:pubId": "yarn prebuild && node nodescripts/buildprebid.js --suffix=pubid --env=all --modules=userId,pubProvidedIdSystem",
    "build:pubId:DL": "yarn prebuild && node nodescripts/buildprebid.js --suffix=pubid --version=4.13.0 --env=all --modules=userId,pubProvidedIdSystem",
    "dist": "npm run build && npm run build:prod",
    "dist:move": "npm run dist && npm run move",
    "move": "node nodescripts/buildprebid.js --move",
    "prebuild": "npm run prettier && npm run tslint && npm run rollup",
    "prettier": "prettier \"./src/*.ts\" --write",
    "rollup": "rollup -c",
    "tslint": "tslint -c tslint.json \"./src/**/*.ts\""
  },
  "author": "jesper.h.tegner@jppol.dk",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "cpx": "^1.5.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.28.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^4.0.5",
    "uglify-js": "^3.11.4"
  }
}
