{
  "name": "jppol-sharedprebid",
  "version": "3.24.3",
  "description": "prebid for eb",
  "main": "index.js",
  "scripts": {
    "build": "npm run uglify-debug",
    "dist": "npm run build && npm run uglify-prod",
    "dist:move": "npm run dist && npm run move_to_eb",
    "prebid": "node buildprebid.js",
    "prebid-update": "npm uninstall --save prebid.js && npm i --save prebid.js",
    "prebid-withupdate": "npm run prebid-update && node buildprebid.js",
    "prebuild": "npm run prettier && npm run tslint && npm run rollup",
    "prettier": "prettier \"./src/*.ts\" --write",
    "rollup": "rollup -c",
    "uglify-prod": "uglifyjs --config-file \"./uglify.prod.config.json\" \"./prebid/prebid3.24.0.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.min.js\"",
    "uglify-debug": "uglifyjs --config-file \"./uglify.dev.config.json\" \"./prebid/prebid3.24.0.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.dev.js\"",
    "tslint": "tslint -c tslint.json \"./src/**/*.ts\"",
    "move_to_eb": "yarn cpx \"./jppol/**/*.{css,js}\" \"../ekstrabladet/ekstrabladet-publication/src/main/webapp/assets/prebid-v3\""
  },
  "author": "jesper.h.tegner@jppol.dk",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "cpx": "^1.5.0",
    "prettier": "^2.0.5",
    "rollup": "^2.23.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.9.7",
    "uglify-js": "^3.10.1"
  }
}
