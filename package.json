{
  "name": "eb_banners",
  "version": "1.36.0",
  "description": "prebid for eb",
  "main": "index.js",
  "scripts": {
    "build": "npm run uglify-debug",
    "dist": "npm run build && npm run uglify-prod",
    "prebid": "node buildprebid.js",
    "prebid-update": "npm uninstall --save prebid.js && npm i --save prebid.js",
    "prebid-withupdate": "npm run prebid-update && node buildprebid.js",
    "prebuild": "npm run prettier && npm run tslint && npm run rollup",
    "prettier": "prettier \"./src/*.ts\" --write",
    "rollup": "rollup -c",
    "uglify-prod": "uglifyjs --config-file=\"./uglify.prod.config.json\" \"./prebid/prebid.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.min.js\"",
    "uglify-debug": "uglifyjs --config-file=\"./uglify.dev.config.json\" \"./prebid/prebid.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.dev.js\"",
    "tslint": "tslint -c tslint.json \"./src/**/*.ts\""
  },
  "author": "jesper.h.tegner@jppol.dk",
  "dependencies": {
    "prebid.js": "^1.36.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "prettier": "^1.14.3",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-typescript2": "^0.18.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.5",
    "uglify-js": "^3.4.9"
  }
}
