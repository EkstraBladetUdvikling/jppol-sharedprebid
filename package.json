{
  "name": "jppol-sharedprebid",
  "version": "3.19.0",
  "description": "prebid for eb",
  "main": "index.js",
  "scripts": {
    "build": "npm run uglify-debug",
    "dist": "npm run build && npm run uglify-prod",
    "prebid": "node buildprebid.js",
    "prebid-update": "npm uninstall --save prebid.js && npm i --save prebid.js",
    "prebid-withupdate": "npm run prebid-update && node buildprebid.js",
    "prebuild": "npm run prettier && npm run tslint && npm run rollup",
    "prettier": "prettier \"./src/*.ts\" --write",
    "rollup": "rollup -c",
    "uglify-prod": "uglifyjs --config-file=\"./uglify.prod.config.json\" \"./prebid/prebid3.19.0.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.min.js\"",
    "uglify-debug": "uglifyjs --config-file=\"./uglify.dev.config.json\" \"./prebid/prebid3.19.0.js\" \"./tspublic/index.js\" -o \"./jppol/jppol-prebid.dev.js\"",
    "tslint": "tslint -c tslint.json \"./src/**/*.ts\"",
    "move_to_eb": "yarn cpx \"./jppol/**/*.{css,js}\" \"../ekstrabladet/ekstrabladet-publication/src/main/webapp/assets/prebid-v3\""
  },
  "author": "jesper.h.tegner@jppol.dk",
  "dependencies": {
    "prebid.js": "2.44.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "cpx": "^1.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.7.4",
    "uglify-js": "^3.7.4"
  }
}
