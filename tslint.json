{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": [false, "log", "error"],
    "interface-name": [true, "always-prefix"],
    "no-unused-expression": [true, "allow-fast-null-checks", "allow-new"],
    "prefer-for-of": false
  },
  "rulesDirectory": []
}
